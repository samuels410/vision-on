<header class="navbar  navbar-fixed-top ">
  <div class="navbar-inner">
    <div class="container">
      <h4 id="logo"><%= link_to(image_tag("logo.png"), root_path) %></h4>
      <nav>
        <ul class="nav pull-right" style="margin-top: 20px;">
          |<a href="#galary" id="galary-link" class="btn" >Collections</a>
          <li><%= breadcrumbs %></li>
          <%= yield :action_links %> |
          <% if current_user  %>
